{
  "extends": ["react-app", "plugin:jsdoc/recommended"],
  "plugins": ["jsdoc"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off",
    "jsdoc/require-jsdoc": 0,
    "jsdoc/no-undefined-types": 0
  },
  "settings": {
    "jsdoc": {
      "mode": "typescript"
    }
  }
}
