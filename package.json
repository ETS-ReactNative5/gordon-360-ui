{
  "name": "gordon-360-ui",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@date-io/moment": "^2.10.6",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.8",
    "caniuse-lite": "^1.0.30001137",
    "chart.js": "^2.9.3",
    "classnames": ">=2.2.5",
    "cropperjs": "^1.5.9",
    "date-fns": "^2.16.1",
    "downshift": "^4.1.0",
    "history": "^4.10.1",
    "http-proxy-middleware": "^0.20.0",
    "interweave": "^12.5.0",
    "jwt-decode": ">=2.2.0",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "moment": "^2.29.0",
    "prop-types": ">=15.6.0",
    "react": "^16.13.1",
    "react-big-calendar": "^0.28.0",
    "react-chartjs-2": "^2.10.0",
    "react-cropper": "^2.1.4",
    "react-document-title": ">=2.0.3",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.0",
    "react-ga": "^3.1.2",
    "react-graceful-image": "^1.5.0",
    "react-icons": "^3.11.0",
    "react-image-gallery": "^1.0.8",
    "react-media": ">=1.8.0",
    "react-responsive-carousel": "^3.2.10",
    "react-router": "~5.0.1",
    "react-router-dom": "~5.0.1",
    "react-timeago": ">=4.4.0",
    "typescript": "^4.0.3",
    "url-regex": ">=5.0.0"
  },
  "devDependencies": {
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "husky": "^4.3.0",
    "jasmine-core": "^3.6.0",
    "jsdoc": "^3.6.6",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.3.1",
    "karma-safari-launcher": ">=1.0.0",
    "lint-staged": "^8.0.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": ">=4.1.2",
    "prettier": "^2.1.2",
    "react-scripts": "^3.4.3",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended-scss": ">=3.0.0",
    "stylelint-scss": "^3.18.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all --parallel watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docs": "jsdoc --configure .jsdoc.json --recurse",
    "format": "prettier --write '{**/*.md,src/**/*.{js,scss}}'",
    "lint": "npm-run-all --parallel --aggregate-output --silent lint-js lint-scss",
    "lint-js": "eslint --ext .js src",
    "lint-scss": "stylelint 'src/**/*.scss'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{**/*.md,src/**/*.{js,scss}}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
